<% function currencySymbol() { switch(currency.code) { case 'EUR' : return 'â‚¬' ; case 'USD' : return '$' ; case 'SEK' :
    return 'kr' ; case 'XOF' : return 'CFA' ; default: return '' ; } } %>

    <div class="container pick-hosting">
        <div class="col-md-12 mb-5">
            <div class="d-flex justify-content-end">
                <div id="CurrComponent"></div>
            </div>
            <header>
                <div class="pricing-header p-3 pb-md-4 mx-auto text-center">
                    <h1 class="display-4 fw-normal">Our Logo Plans</h1>
                    <p class="fs-5 text-muted">
                        Lorem ipsum dolor sit amet consectetur adipisicing elit. Modi est explicabo eum ad ea, impedit
                        itaque
                        aliquid tempora quo reprehenderit obcaecati, quibusdam necessitatibus, ab consequuntur.
                    </p>
                </div>
            </header>
            <div class="row row-cols-1 row-cols-md-3 mb-3 text-center">
                <% plans.forEach((plan, i)=> { %>
                    <div class="col">
                        <div class="card mb-4 rounded-3 shadow-sm">
                            <div class="card-header py-3 <%= i === 1 ? 'bg-primary text-white' : '' %>">
                                <h4 class="my-0 fw-normal text-capitalize">
                                    <%= plan.title %>
                                </h4>
                            </div>
                            <div class="card-body">
                                <h1 class="card-title pricing-card-title">
                                    <%= currencySymbol() %>
                                        <%= Math.round(plan.price) %>
                                </h1>
                                <ul class="list-unstyled mt-3 mb-4">
                                    <% plan.includes.forEach(item=> { %>
                                        <li class="mb-1">
                                            <%= item %>
                                        </li>
                                        <% }) %>
                                </ul>
                                <form method="POST" action="/build-a-logo">
                                    <input type="hidden" name="plan" value="<%= plan._id %>">
                                    <button type="submit"
                                        class="w-100 btn btn-lg <%= i === 1 ? 'btn-primary' : 'btn-outline-primary' %>">
                                        Choose Plan
                                    </button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <% }) %>
            </div>
        </div>

        <div class="col-md-12">
            <table class="table">
                <thead>
                    <tr>
                        <th class="large text-left"></th>
                        <th class="text-capitalize">Basic Logo Plan</th>
                        <th class="text-capitalize">Launch Logo Plan</th>
                        <th class="text-capitalize">Designer Logo Plan</th>
                    </tr>
                </thead>

                <tbody>
